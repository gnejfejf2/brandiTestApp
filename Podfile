# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'BrandiApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BrandiApp

  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'RxGesture'
  pod 'Moya/RxSwift'
  pod 'Then'
  # Networking
  pod 'Alamofire'
  
  pod 'Kingfisher'
  
  # UI
  pod 'SnapKit'

  target 'BrandiAppTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RxSwift'
    
    
    pod 'Moya/RxSwift'
    pod 'RxTest'
  end

end
